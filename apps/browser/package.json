{
  "name": "@frw/browser",
  "version": "1.0.0",
  "description": "FRW Browser - Decentralized web browser",
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "vite",
    "electron:build": "vite build && electron-builder"
  },
  "keywords": [
    "frw",
    "browser",
    "electron",
    "decentralized"
  ],
  "author": "FRW Community",
  "license": "MIT",
  "dependencies": {
    "@frw/common": "1.0.0",
    "@frw/crypto": "1.0.0",
    "@frw/ipfs": "1.0.0",
    "@frw/protocol": "1.0.0",
    "@types/node-fetch": "^2.6.13",
    "node-fetch": "^2.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.frw.browser",
    "productName": "FRW Browser",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}

{
  "name": "@frw/browser",
  "version": "2.0.0",
  "description": "FRW Browser V2 - Quantum-Resistant Decentralized Web Browser",
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "start": "electron dist/main/index.js",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && npx tsc -p tsconfig.build.json && electron dist/main/index.js",
    "build": "vite build && npx tsc -p tsconfig.build.json && electron-builder",
    "build:vite": "vite build",
    "build:electron": "npx tsc -p tsconfig.build.json",
    "build:dist": "electron-builder"
  },
  "keywords": [
    "frw",
    "browser",
    "electron",
    "decentralized"
  ],
  "author": "FRW Community",
  "license": "MIT",
  "dependencies": {
    "@frw/common": "1.0.1",
    "@frw/crypto": "1.0.1",
    "@frw/crypto-pq": "2.0.0",
    "@frw/ipfs": "1.0.1",
    "@frw/name-registry": "1.0.1",
    "@frw/protocol": "1.0.1",
    "@frw/protocol-v2": "2.0.0",
    "@frw/pow-v2": "2.0.0",
    "@types/node-fetch": "^2.6.13",
    "node-fetch": "^2.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "28.0.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.frw.browser",
    "productName": "FRW Browser",
    "npmRebuild": false,
    "asar": false,
    "directories": {
      "output": "release-v2"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64"]
        },
        {
          "target": "portable",
          "arch": ["x64"]
        }
      ],
      "sign": null
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
